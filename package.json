{
  "name": "mail-api-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/www.js",
    "prestart": "rm -v -rf dist && parcel build bin/www.ts -d dist --target node  && mv -i -v src/public dist/ && mv -i -v src/views dist/ && rm -v -rf src",
    "dev:watch": "nodemon --watch src --delay 1 --exec ts-node bin/www.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.5",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "v12.18.1"
  }
}
